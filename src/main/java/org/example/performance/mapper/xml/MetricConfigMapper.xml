<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.performance.mapper.MetricConfigMapper">

    <resultMap id="BaseResultMap" type="org.example.performance.pojo.po.MetricConfig">
        <id property="id" column="id" jdbcType="INTEGER"/>
        <result property="metricName" column="metric_name" jdbcType="VARCHAR"/>
        <result property="type" column="type" jdbcType="CHAR"/>
        <result property="metricDesc" column="metric_desc" jdbcType="VARCHAR"/>
        <result property="origin" column="origin" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,metric_name,type,
        metric_desc,origin
    </sql>
</mapper>
